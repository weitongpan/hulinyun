<!--pages/index/chlidrencpn/swiper/swiper.wxml-->
<swiper class="swiper" indicator-dots indicator-color="rgba(255, 255, 255, .3)" indicator-active-color="#ffffff" autoplay circular>
  <block wx:for="{{swiper}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image class="swiper-img" src="{{item}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>